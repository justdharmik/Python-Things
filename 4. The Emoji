#******THE EMOJI******

print("""
Tips : 
:) - Smiley
:( - Sad
xD - Laugh
-_- - Groan
<3 - Heart
""")

message = input('> ')
words = message.split(' ')                              #---split-method-will-break-the-string-into-parts-wherever-it-finds-space-' '
                                                        #---printing-this-will-return-a-list

emojis = {
    ":)" : "ğŸ™‚",
    ":(" : "ğŸ™", 
    "xD" : "ğŸ¤£",
    "-_-" : "ğŸ¤",
    "<3" : "ğŸ’“"
}
output = ""

for word in words:
    output += emojis.get(word, word) + " "
print(output)

